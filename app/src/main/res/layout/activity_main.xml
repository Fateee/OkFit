        android:textSize="50dp"
        android:text="12333"
        />

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Server"
        android:background="@color/colorPrimary"
        android:textColor="#ffffff"
        android:textSize="50dp"
        />

</LinearLayout>
